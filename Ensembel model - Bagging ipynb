   ],
   "source": [
    "emptydf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "#bootstrap sample size\n",
    "n = 25\n",
    "\n",
    "#creating a empty dataframe with n columns\n",
    "\n",
    "\n",
    "for i in range(0,n):\n",
    "    #Extracting sample from the dataset of length same as dataset with replacement\n",
    "    sample = iris.sample(len(iris), replace=True)\n",
    "    #Finding out the unique index\n",
    "    unique = list(sample.index.unique())\n",
    "    remaining = list(iris.index)\n",
    "    #finding out the index which are left out of sampling and these left out samples are used as test data\n",
    "    #remaining =[]\n",
    "    for j in unique:\n",
    "    #print(i)\n",
    "        remaining.remove(j)\n",
    "            \n",
    "    # splitting x train and y train\n",
    "    X_train = sample.iloc[0:len(sample),0:4]\n",
    "    y_train = sample.iloc[0:len(sample),4:]\n",
    "\n",
    "    #fitting the decision tree with the training data\n",
    "    clf_gini = DecisionTreeClassifier(criterion = 'gini', random_state = 100)\n",
    "    clf_gini.fit(X_train,y_train)\n",
    "    \n",
    "    #splitting x test and y test\n",
    "    X_test = iris.iloc[remaining,0:4]\n",
    "    y_test = iris.iloc[remaining,4:]\n",
    "    \n",
    "    #predicting the value of y with X test\n",
    "    y_pred = clf_gini.predict(X_test)\n",
    "    \n",
    "    #adding the predicted test data points to the dataframe index position\n",
    "    x=0\n",
    "    for k in remaining:\n",
    "        emptydf.loc[k,i] = y_pred[x]\n",
    "        x=x+1\n",
    "\n",
    "#emptydf\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 31,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
